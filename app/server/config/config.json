{
  "publicDir": "/frontend/public",
  "redis": {
    "host": "redis",
    "port": 6379
  },
  "firewall": {
    "secretKey": "my-secret",
    "tokenOptions": {
      "expiresIn": "1h"
    }
  },
  "authResolver": {
    "cookieOption": {
      "httpOnly": true
    }
  },
  "createPostHandler": {
    "domain": "https://storage.googleapis.com/media-bucket-0/",
    "fileError": "Not supported file type"
  },

  "mysqlDatabaseBackupCron": {
    "days": 1,
    "backupFileName": "databases-backup.sql"
  }
}
